#include <iostream>
#include "UPoliL.h"

using namespace std;

int main() {
	PoliL* p = new PoliL();
	int v[] = {-1, 1, 3, 2, 4, 1, -3, 1};
	cout << p->to_str() << endl;
	for (int i = 0; i < 6; i += 2) {
		p->poner_termino(v[i], v[i + 1]);
		cout << p->to_str() << endl;
	}
	system("pause");
	return 0;
}
